<?xml version="1.0"?>
<extension key="at.greenpeace.mailingwork" type="module">
  <file>mailingwork</file>
  <name>Mailingwork</name>
  <description>API Integration with Mailingwork</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Patrick Figel</author>
    <email>pfigel@greenpeace.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/greenpeace-cee/at.greenpeace.mailingwork</url>
    <url desc="Documentation">https://github.com/greenpeace-cee/at.greenpeace.mailingwork</url>
    <url desc="Support">https://github.com/greenpeace-cee/at.greenpeace.mailingwork/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-08-21</releaseDate>
  <version>1.4.0-beta.2</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Mailingwork</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Mailingwork_Upgrader</upgrader>
</extension>
